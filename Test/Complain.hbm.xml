<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Test.Model.Complain, Test.Model" table="t_complain">
    <id name="Id" type="Int32" unsaved-value="null">
      <column name="t_id" sql-type="int" not-null="true" unique="true" index="Pk_Grade"/>
      <generator class="native" />
    </id>
    <property name="Message" type="string">
      <column name="t_message" sql-type="varchar" not-null="false"/> 
    </property>
    <property name="Dtime" type="DateTime">
      <column name="t_dtime" sql-type="datetime" not-null="false"/>
    </property>
  </class>
</hibernate-mapping>
